<rect 
    class="day_<%= day %> week_<%= week %> contribution level_<%= weeks[week][day]["level"] %>"

    x="<%= week * 13 %>" y="<%= day * 13 %>"
    width="10" height="10"

    transform-origin="<%= week * 13 + 5 %> <%= day * 13 + 5 %>"
    style="animation-delay: <%= day / 15 + week / 50 %>s;"

    rx="2" 
/>